    padding: 20px;
    max-height: 80vh;
    overflow: auto;
    
}

.smart-modal-window.smart-modal-small {
    width: 30%;
}

.smart-modal-window.smart-modal-large {
    width: 75%;
}

.smart-modal-close {
    position: absolute;
    top: 0;
    right: 0;
    color: rgba(0,0,0,0.3);
    height: 30px;
    width: 30px;
    font-size: 30px;
    line-height: 30px;
    text-align: center;
}

.smart-modal-close:hover,
.smart-modal-close:focus {
    color: #000000;
    cursor: pointer;
}

.smart-modal-open {
    display: block;
}

@media (max-width: 767px) {
